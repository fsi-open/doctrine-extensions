{
    "name": "fsi/doctrine-extensions",
    "type": "library",
    "description": "FSi Behavioral Doctrine Extensions",
    "keywords": ["fsi", "doctrine-extensions", "doctrine", "orm", "translatable", "uploadable"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        },
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "doctrine/common": "^2.9|^3.0",
        "doctrine/orm": "^2.5",
        "doctrine/persistence": "^1.3|^2.0",
        "symfony/property-access": "^3.0|^4.0|^5.0",
        "symfony/yaml": "^3.0|^4.0|^5.0",
        "knplabs/gaufrette": "~0.1,>=0.1.4"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-pdo_sqlite": "*",
        "ext-simplexml": "*",
        "doctrine/dbal": "^2.5",
        "doctrine/doctrine-bundle": "^1.3|^2.0",
        "phpstan/phpstan": "^0.12.26",
        "phpunit/phpunit": "^7.5|^8.0",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "FSi\\DoctrineExtensions\\": "lib/FSi/DoctrineExtensions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\DoctrineExtensions\\Tests\\": "tests/FSi/DoctrineExtensions/Tests/"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.2": "1.2-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
